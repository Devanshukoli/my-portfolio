{
  "name": "my-portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Portfolio website monorepo with Vue.js frontend and Express backend",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev -w packages/frontend",
    "server": "npm run server -w packages/backend",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "npm run build -w packages/frontend",
    "preview": "npm run preview -w packages/frontend",
    "format": "prettier --write .",
    "storybook": "npm run storybook -w packages/frontend",
    "build-storybook": "npm run build-storybook -w packages/frontend"
  },
  "keywords": [],
  "author": "Devanshukoli",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.6.0"
  }
}
